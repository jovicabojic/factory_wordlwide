KAAK,CAACG,KAAN,GAAcV,UAAU,CAACW,UAAnC;AACA,UAAMK,OAAO,GAAG,CAACD,CAAC,GAAGX,MAAL,IAAe,CAA/B;AACAJ,MAAAA,UAAU,CAACY,UAAX,GAAwBP,aAAa,GAAGW,OAAxC;AACD,KAND;AAOD;AACF,CA5BD","sourcesContent":["module.exports = (elementName)=>{\n  let MainContId = document.getElementById(`${elementName}`);\n  let isDown = false;\n  let startX;\n  let scrollLeftVal;\n  if (MainContId) {\n    MainContId.addEventListener('mousedown', (event) => {\n      isDown = true;\n      MainContId.classList.add('active-drag');\n      startX = event.pageX - MainContId.offsetLeft;\n      scrollLeftVal = MainContId.scrollLeft;\n    });\n    MainContId.addEventListener('mouseleave', () => {\n      isDown = false;\n      MainContId.classList.remove('active-drag');\n    });\n    MainContId.addEventListener('mouseup', () => {\n      isDown = false;\n      MainContId.classList.remove('active-drag');\n    });\n    MainContId.addEventListener('mousemove', (event) => {\n      if(!isDown) return;\n      event.preventDefault();\n      const x = event.pageX - MainContId.offsetLeft;\n      const goValue = (x - startX) * 3;\n      MainContId.scrollLeft = scrollLeftVal - goValue;\n    });\n  }\n}\n"]}]}